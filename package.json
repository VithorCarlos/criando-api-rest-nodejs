{
  "name": "api-rest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "20.11.0"
  },
  "scripts": {
    "dev": "tsnd --transpile-only  --respawn --ignore-watch node_modules src/server.ts",
    "migrate:down": "npx knex migrate:down --ext ts",
    "migrate:drop": "npx ts-node ./node_modules/.bin/knex migrate:rollback",
    "migrate:up": "npx ts-node ./node_modules/.bin/knex migrate:latest",
    "migrate:make": "npx knex migrate:make --ext ts",
    "lint": "eslint src --ext .ts --fix",
    "build": "tsup src --out-dir build",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "ts-node": "^10.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/dotenv": "^8.2.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}